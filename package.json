{
  "name": "must-vscode",
  "displayName": "must-vscode",
  "description": "markup language support tool",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:must-vscode.urlToLink"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "must-vscode.urlToLink",
        "title": "must: url to Link"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "jest": "yarn compile && jest --roots=./out/jest",
    "test": "jest --roots=./out/test"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "17.0.42",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vscode/test-electron": "^2.1.4",
    "eslint": "^8.17.0",
    "glob": "^8.0.3",
    "jest": "^28.1.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "jsdom": "^19.0.0"
  }
}
